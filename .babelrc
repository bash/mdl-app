{
  "comments": false,
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "last 1 ios_saf version",
            "last 1 opera version",
            "last 1 edge version"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "external-helpers",
    "transform-node-env-inline",
    "transform-object-rest-spread",
    "transform-preact-import",
    [
      "transform-react-jsx",
      { "pragma": "h" }
    ]
  ]
}
